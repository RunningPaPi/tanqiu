			connectionURL="jdbc:mysql://127.0.0.1:3306/tanqiu?useUnicode=true&amp;characterEncoding=UTF-8"
			userId="root" password="123456" />
        -->
        
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://106.75.173.117:13306/tanqiu?useUnicode=true&amp;characterEncoding=UTF-8"
			userId="root" password="k8U@*hy4icomxz" />


		<!-- java type resolver -->
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- gen entity -->
		<!--data -->
		<javaModelGenerator
			targetPackage="com.artqiyi.tanqiu.redpack.domain"
			targetProject="./src/main/java">
			<property name="enableSubPackages" value="false" />
			<property name="trimStrings" value="false" />
		</javaModelGenerator>

		<!-- gen annotated mapper -->
		<!-- data -->
		<javaClientGenerator type="ANNOTATEDMAPPER"
			targetPackage="com.artqiyi.tanqiu.redpack.mapper"
			targetProject="./src/main/java">
			<property name="enableSubPackages" value="false" />
		</javaClientGenerator>

		<!---->

		<table tableName="redpack_group_task">
			<generatedKey column="group_task_id" sqlStatement="JDBC"
				identity="true" />
		</table>
		
		<table tableName="redpack_group_task_log">
			<generatedKey column="group_task_log_id" sqlStatement="JDBC"
				identity="true" />
		</table>		

		<table tableName="redpack_task">
			<generatedKey column="task_id" sqlStatement="JDBC"
				identity="true" />
		</table>
		
		<table tableName="redpack_task_log">
			<generatedKey column="task_log_id" sqlStatement="JDBC"
				identity="true" />
		</table>	

		<table tableName="redpack_withdraw_application">
			<generatedKey column="application_id" sqlStatement="JDBC"
				identity="true" />
		</table>	

	</context>
</generatorConfiguration>