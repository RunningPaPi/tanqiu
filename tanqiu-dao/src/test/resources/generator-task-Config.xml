<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>

	<context id="entmobile" targetRuntime="MyBatis3">

		<plugin type="org.mybatis.generator.plugins.AddLimitOffsetPlugin" />

		<commentGenerator>
			<property name="suppressDate" value="true" />
			<!-- remove comments -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>

		<!-- //////////////////////////////////////////////////////////////////////////////////////////////////////////////// -->
		<!-- jdbc connection -->
		<!-- data -->

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://127.0.0.1:3306/tanqiu?useUnicode=true&amp;characterEncoding=UTF-8" userId="root"
			password="123456" />


		<!-- java type resolver -->
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- gen entity -->
		<!--data -->
		<javaModelGenerator targetPackage="com.artqiyi.tanqiu.task.domain"
			targetProject="./src/main/java">
			<property name="enableSubPackages" value="false" />
			<property name="trimStrings" value="false" />
		</javaModelGenerator>

		<!-- gen annotated mapper -->
		<!-- data -->
		<javaClientGenerator type="ANNOTATEDMAPPER"
			targetPackage="com.artqiyi.tanqiu.task.mapper" targetProject="./src/main/java">
			<property name="enableSubPackages" value="false" />
		</javaClientGenerator>

		<!--
		<table  tableName="task_list" >
		    <generatedKey column="task_id" sqlStatement="JDBC" identity="true" />
		</table>
		<table  tableName="task_list_reward_rule_map" >

		</table>

		<table  tableName="task_reward_rule" >
			<generatedKey column="reward_rule_id" sqlStatement="JDBC" identity="true" />
		</table>
		-->

		<table  tableName="task_log" >
			<generatedKey column="task_log_id" sqlStatement="JDBC" identity="true" />
		</table>

	</context>
</generatorConfiguration>